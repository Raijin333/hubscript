local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/Kronos"))()

local window = library:Window({
   Title = "Raijins Script Libary - Made By Raijin and Ent ‚ù§",
   Accent = Color3.fromRGB(250,250,250),
   Logo = 2682121931,
   ToggleKey = Enum.KeyCode.LeftAlt
})

local tab = window:NewTab({
   Logo = 4483345998
})

local tabsection = tab:TabSection({
   Title = "Anime Games"
})

local column = tabsection:AddColumn({
   Title = "PVP Games"
})






-- Blox Fruits




local section = column:Section({
   Title = "Blox Fruits"
})

section:Button({
   Text = "Makuro Hub (Key or No Key)",
   Callback = function()
       loadstring(game:HttpGet"https://raw.githubusercontent.com/xQuartyx/DonateMe/main/ScriptLoader")()
   end
})

section:Button({
   Text = "HoHo Hub (Key or No Key)",
   Callback = function()
       loadstring(game:HttpGet('https://raw.githubusercontent.com/acsu123/HOHO_H/main/Loading_UI'))()
   end
})

section:Button({
   Text = "Neva Hub (No Key)",
   Callback = function()
       loadstring(game:HttpGet('https://raw.githubusercontent.com/acsu123/HOHO_H/main/Loading_UI'))()
   end
})

section:Button({
   Text = "Mango Hub (No Key)",
   Callback = function()
       getgenv().WaterMark = false
loadstring(game:HttpGet("https://gitlab.com/L1ZOT/mango-hub/-/raw/main/Mango-Bloxf-Fruits-Beta"))()
   end
})


